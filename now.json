{
  "version": 2,
  "name": "freiluft-festival",
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {}
    }
  ],
  "regions": ["bru1"],
  "public": true,
  "github": {
    "enabled": true,
    "silent": false
  },
  "routes": [
    {
      "src": "/_nuxt/.+",
      "headers": {
        "cache-control": "s-maxage=31536000",
        "X-Robots-Tag": "none"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Robots-Tag": "none"
      },
      "dest": "/"
    }
  ]
}