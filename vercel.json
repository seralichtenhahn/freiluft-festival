{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "maxLambdaSize": "100mb"
      }
    }
  ],
  "regions": ["bru1"],
  "public": true,
  "github": {
    "enabled": true,
    "silent": false
  },
  "env": {
    "STORYBLOK_TOKEN": "@storyblok-token-draft",
    "STORYBLOK_ONLY_PUBLIC": "0",
    "RUN_WITH_SSR": "true",
    "STORYBLOK_VERSION": "draft"
  },
  "build": {
    "env": {
      "STORYBLOK_TOKEN": "@storyblok-token-draft",
      "STORYBLOK_ONLY_PUBLIC": "0",
      "RUN_WITH_SSR": "true",
      "STORYBLOK_VERSION": "draft"
    }
  },
  "routes": [
    {
      "src": "/_nuxt/.+",
      "headers": {
        "cache-control": "s-maxage=31536000",
        "X-Robots-Tag": "none"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Robots-Tag": "none"
      },
      "dest": "/"
    }
  ]
}